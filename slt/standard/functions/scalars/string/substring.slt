# SUBSTRING

query T
SELECT substring('hello', 1);
----
hello

query T
SELECT substring('hello', 2);
----
ello

query T
SELECT substring('hello', 1, 3);
----
hel

query T
SELECT substring('hello', 2, 3);
----
ell

query T
SELECT substring('😤🤝👀', 2);
----
🤝👀

query T
SELECT substring('😤🤝👀', 2, 1);
----
🤝

